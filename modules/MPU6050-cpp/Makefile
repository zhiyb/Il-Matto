# MPU6050 port connection
MPU6050_PORT	?= D
MPU6050_PORT_SCL	?= 0
MPU6050_PORT_SDA	?= 1
CONFS	+= MPU6050_PORT MPU6050_PORT_SCL MPU6050_PORT_SDA

I2C_PORT	= $(MPU6050_PORT)
I2C_PORT_SCL	= $(MPU6050_PORT_SCL)
I2C_PORT_SDA	= $(MPU6050_PORT_SDA)

# File rules
NSRC	:= mpu6050.cpp
SRC	+= $(NSRC:%=$(DIR)/%)
INCDIRS	+= $(DIR)
